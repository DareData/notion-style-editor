{
  "name": "altos-react-text-editor",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/altos-react-text-editor.umd.cjs",
  "module": "./dist/altos-react-text-editor.js",
  "exports": {
    ".": {
      "import": "./dist/altos-react-text-editor.js",
      "require": "./dist/altos-react-text-editor.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vite build",
    "build:pages": "rm -rf docs && vite build --config vite.page.config.ts",
    "preview": "vite preview",
    "lint": "yarn run lint:js",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:style:fix",
    "lint:js": "eslint --ext .ts,.tsx ./src",
    "lint:js:fix": "npm run lint -- --fix",
    "lint:style": "stylelint './src/**/*.tsx'",
    "lint:style:fix": "stylelint './src/**/*.tsx' --fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@milkdown/core": "^7.0.1",
    "@milkdown/ctx": "^7.0.1",
    "@milkdown/plugin-clipboard": "^7.1.1",
    "@milkdown/plugin-diagram": "^7.1.0",
    "@milkdown/plugin-emoji": "^7.1.1",
    "@milkdown/plugin-history": "^7.0.1",
    "@milkdown/plugin-listener": "^7.0.1",
    "@milkdown/plugin-math": "^7.1.0",
    "@milkdown/plugin-prism": "^7.0.1",
    "@milkdown/plugin-slash": "^7.1.0",
    "@milkdown/plugin-tooltip": "^7.1.0",
    "@milkdown/plugin-trailing": "^7.1.1",
    "@milkdown/plugin-upload": "^7.1.0",
    "@milkdown/preset-commonmark": "^7.0.1",
    "@milkdown/preset-gfm": "^7.0.1",
    "@milkdown/prose": "^7.0.1",
    "@milkdown/react": "^7.0.1",
    "@milkdown/transformer": "^7.0.1",
    "@prosemirror-adapter/react": "^0.2.2",
    "framer-motion": "^10.2.5",
    "prosemirror-view": "^1.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropdown": "^1.11.0",
    "react-hook-form": "^7.43.5",
    "react-hot-toast": "^2.4.0",
    "styled-components": "^5.3.9",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.14.5",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.3.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "yarn run lint:js:fix",
      "yarn run lint:style:fix"
    ]
  }
}
